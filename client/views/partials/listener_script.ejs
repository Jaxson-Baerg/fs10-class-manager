<script defer>
  $('.mobile-nav-toggle').on('click', () => {
    $('#navbar').toggleClass('navbar-mobile');
    const mobileNavBtn = $('.mobile-nav-toggle');
    mobileNavBtn.toggleClass('fa-bars');
    mobileNavBtn.toggleClass('bi-list bi-x fa-xmark');
  });
  $('.nav-link').on('click', () => {
    $('#navbar').toggleClass('navbar-mobile', false);
    const mobileNavBtn = $('.mobile-nav-toggle');
    mobileNavBtn.toggleClass('fa-bars', true);
    mobileNavBtn.toggleClass('bi-list bi-x fa-xmark', false);
  });
</script>
